{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
    "extends": [
      "config:base",
      ":semanticCommitTypeAll(chore)"
    ],
  "regexManagers": [
    {
      "fileMatch": ["^.github/workflows/generate.yml$"],
      "matchStrings": ["https://raw.githubusercontent.com/Sonarr/Sonarr/(?<currentDigest>.*?)\\n"],
      "currentValueTemplate": "develop",
      "depNameTemplate": "Sonarr",
      "datasourceTemplate": "git-refs"
    },
    {
      "fileMatch": ["^.github/workflows/generate.yml$"],
      "matchStrings": ["https://raw.githubusercontent.com/Radarr/Radarr/(?<currentDigest>.*?)\\n"],
      "currentValueTemplate": "develop",
      "depNameTemplate": "Radarr",
      "datasourceTemplate": "git-refs"
    },
    {
