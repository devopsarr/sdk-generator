{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base",
    ":semanticCommitTypeAll(chore)"
  ],
  "regexManagers": [
    {
      "fileMatch": [
        "^.github/workflows/generate.yml$"
      ],
      "matchStrings": [
        "https://raw.githubusercontent.com/Sonarr/Sonarr/(?<currentValue>.*?)/src/Sonarr.Api.V3/openapi.json\\n"
      ],
      "datasourceTemplate": "github-releases",
      "depNameTemplate": "Sonarr/Sonarr"
    },
    {
      "fileMatch": [
        "^.github/workflows/generate.yml$"
      ],
      "matchStrings": [
        "https://raw.githubusercontent.com/Radarr/Radarr/(?<currentValue>.*?)/src/Radarr.Api.V3/openapi.json\\n"
      ],
      "datasourceTemplate": "github-releases",
      "depNameTemplate": "Radarr/Radarr"
    },
    {
      "fileMatch": [
        "^.github/workflows/generate.yml$"
      ],
      "matchStrings": [
        "https://raw.githubusercontent.com/Lidarr/Lidarr/(?<currentValue>.*?)/src/Lidarr.Api.V1/openapi.json\\n"
      ],
      "datasourceTemplate": "github-releases",
      "depNameTemplate": "Lidarr/Lidarr"
    },
    {
      "fileMatch": [
        "^.github/workflows/generate.yml$"
      ],
      "matchStrings": [
        "https://raw.githubusercontent.com/Readarr/Readarr/(?<currentValue>.*?)/src/Readarr.Api.V1/openapi.json\\n"
      ],
      "datasourceTemplate": "github-releases",
      "depNameTemplate": "Readarr/Readarr"
    },
    {
      "fileMatch": [
        "^.github/workflows/generate.yml$"
      ],
      "matchStrings": [
        "https://raw.githubusercontent.com/Prowlarr/Prowlarr/(?<currentValue>.*?)/src/Prowlarr.Api.V1/openapi.json\\n"
      ],
      "datasourceTemplate": "github-releases",
      "depNameTemplate": "Prowlarr/Prowlarr"
    },
    {
      "fileMatch": [
        "^.github/workflows/generate.yml$"
      ],
      "matchStrings": [
        "https://raw.githubusercontent.com/Whisparr/Whisparr/(?<currentDigest>.*?)/src/Whisparr.Api.V3/openapi.json\\n"
      ],
      "currentValueTemplate": "develop",
      "packageNameTemplate": "https://github.com/Whisparr/Whisparr",
      "depNameTemplate": "Whisparr",
      "datasourceTemplate": "git-refs"
    },
    {
      "fileMatch": [
        "^.github/workflows/generate.yml$"
      ],
      "matchStrings": [
        "https://raw.githubusercontent.com/sct/overseerr/(?<currentValue>.*?)/overseerr-api.yml\\n"
      ],
      "datasourceTemplate": "github-releases",
      "depNameTemplate": "sct/overseerr"
    },
    {
      "fileMatch": [
        "^Makefile$"
      ],
      "matchStrings": [
        "OPENAPI_GENERATOR_IMAGE \\?= (?<depName>.*?):(?<currentValue>.*?)@(?<currentDigest>sha256:[a-f0-9]+)"
      ],
      "datasourceTemplate": "docker"
    }
  ]
}